{
	"name": "code-cat",
	"displayName": "Code? Cat!",
	"description": "A VS Code cat displayer inspired by Written Kitten",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"configuration": {
			"title": "Code? Cat!",
			"properties": {
				"codecat.enabled": {
					"default": false,
					"type": "boolean",
					"description": "Enable to activate Code Cat!"
				}
			}
		}
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"axios": "^0.19.0"
	}
}
